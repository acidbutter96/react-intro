props){\n    return (\n        <>\n            <div>Hooks</div>\n            <p>\n                <UserStateTest/>\n            </p>\n            <h2>\n                The last form using Hooks\n            </h2>\n            <FormHook/>\n            <h2>Reuse Hooks</h2>\n            <ReuseHooks/>\n        </>\n    );\n}\n\nexport default Hooks;","/home/marcos/Documents/Celke/estudoNodejs/react-intro/src/FormHooks.js",[],"/home/marcos/Documents/Celke/estudoNodejs/react-intro/src/Css.js",[],"/home/marcos/Documents/Celke/estudoNodejs/react-intro/src/Reactstrap.js",["73"],"import React from \"react\";\nimport {Button} from \"reactstrap\";\n\nexport default (props)=>{\n    return(\n        <Button color=\"danger\" style={{fontSize:40}}>\n            Danger motherfucker\n        </Button>\n    );\n};","/home/marcos/Documents/Celke/estudoNodejs/react-intro/src/routes.js",[],"/home/marcos/Documents/Celke/estudoNodejs/react-intro/src/pages/Main/index.js",[],"/home/marcos/Documents/Celke/estudoNodejs/react-intro/src/pages/Testa/index.js",[],"/home/marcos/Documents/Celke/estudoNodejs/react-intro/src/pages/Teste/index.js",[],{"ruleId":"74","severity":1,"message":"75","line":21,"column":8,"nodeType":"76","messageId":"77","endLine":21,"endColumn":14},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"74","severity":1,"message":"82","line":27,"column":11,"nodeType":"76","messageId":"77","endLine":27,"endColumn":28},{"ruleId":"83","severity":1,"message":"84","line":4,"column":1,"nodeType":"85","endLine":10,"endColumn":3},"no-unused-vars","'Routes' is defined but never used.","Identifier","unusedVar","no-native-reassign",["86"],"no-negated-in-lhs",["87"],"'handleChangeEmail' is assigned a value but never used.","import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","no-global-assign","no-unsafe-negation"]